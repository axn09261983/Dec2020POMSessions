<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Demo Cart App Regression Test Suite" thread-count = "3"
parallel = "tests" verbose="3">


   <listeners>
		<listener class-name="com.qa.opencart.listerners.ExtentReportList" />
		<listener class-name="com.qa.opencart.listerners.TestAllureListener" />
		
	</listeners>



	<test name="Login Test">
		
		<classes>
			<class name="com.qa.opencart.tests.LoginPageTest" />
		</classes>

	</test>
	
 	<test name="AccountPage Test">
		
		<classes>
			<class name="com.qa.opencart.tests.AccountPageTest" />
		</classes>

	</test>
	
	
	<test name="ProductinfoPage Test">
		
		<classes>
			<class name="com.qa.opencart.tests.ProductInfoPageTest" />
		</classes>

	</test>
	
	
	

	
</suite>






